{
  "name": "gql-importer",
  "version": "1.0.4",
  "description": "GraphQL types to typescript interface importer",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "cover": "nyc --reporter html --reporter text npm run test",
    "travis-cover": "nyc --reporter lcovonly npm run test"
  },
  "bin": {
    "gqlImporter": "index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahincalan/gql-importer.git"
  },
